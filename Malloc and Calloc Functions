#include <stdio.h>
#include <stdlib.h>
int main(void)
{
int*p
int sum=0,n;
printf("enter no of array input");
scanf("%d",&n);
P=(int*)malloc(n*sizeof(int));
printf("enter an array of %d elements\n",n);
for(int i=0;i<n;i++)
{
scanf("%d", P+i);
sum = sum+ *(P+i);
}
printf(" Sum of array is %d",sum)
free(P)
return 0;
}
